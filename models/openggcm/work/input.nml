   trusted_obs      = 'null'
   Nregions   = 3
   lonlim1    = 0.0,     0.0,   0.0
   lonlim2    = 360.0, 360.0, 360.0
   latlim1    = -90.0, -90.0,   0.0
   latlim2    =  90.0,   0.0,  90.0
