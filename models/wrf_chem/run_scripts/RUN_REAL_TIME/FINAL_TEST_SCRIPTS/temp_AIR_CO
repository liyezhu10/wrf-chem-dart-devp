#!/bin/ksh -aeux
#BSUB -P P19010000
#BSUB -n 32                       # number of total (MPI) tasks
#BSUB -R "span[ptile=8]"    # mpi tasks per node
#BSUB -J advm_28805                          # job name
#BSUB -o advm_28805.out                      # output filename
#BSUB -e advm_28805.err                      # error filename
#BSUB -W 4:59               # wallclock time (minutes)
