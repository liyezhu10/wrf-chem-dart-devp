#!/bin/ksh -aeux
#BSUB -P P19010000
#BSUB -n 1                                  # number of total (MPI) tasks
#BSUB -J real_29515                          # job name
#BSUB -o real_29515.out                      # output filename
#BSUB -e real_29515.err                      # error filename
#BSUB -W 00:14                              # wallclock time (minutes)
#BSUB -q geyser
#
# Run real
mpirun.lsf ./real.exe  > index.html 2>&1 
#
export RC=$?     
if [[ -f SUCCESS ]]; then rm -rf SUCCESS; fi     
if [[ -f FAILED ]]; then rm -rf FAILED; fi          
if [[ $RC = 0 ]]; then
   touch SUCCESS
else
   touch FAILED 
   exit
fi
EOF
