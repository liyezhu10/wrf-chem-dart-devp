#!/bin/ksh -aeux
#BSUB -P P19010000
#BSUB -n 1
#BSUB -R "span[ptile=8]"
#BSUB -J pert_bc_7598
#BSUB -o pert_bc_7598.out
#BSUB -e pert_bc_7598.err
#BSUB -W 0:30
#BSUB -q geyser
#
mpirun.lsf ./pert_wrf_bc  > index.html 2>&1 
#
export RC=$?     
if [[ -f SUCCESS ]]; then rm -rf SUCCESS; fi     
if [[ -f FAILED ]]; then rm -rf FAILED; fi          
if [[ $RC = 0 ]]; then
   touch SUCCESS
else
   touch FAILED 
   exit
fi
