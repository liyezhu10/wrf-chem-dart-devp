#BSUB -P P19010000
#BSUB -n 32                       # number of total (MPI) tasks
#BSUB -R "span[ptile=8]"    # mpi tasks per node
#BSUB -J advm_7598                          # job name
#BSUB -o advm_7598.out                      # output filename
#BSUB -e advm_7598.err                      # error filename
#BSUB -W 4:59               # wallclock time (minutes)
#BSUB -q geyser
#
mpirun.lsf ./wrf.exe > index_wrfchem_0002 2>&1 
export RC=$?     
rm -rf WRFCHEM_SUCCESS_*
rm -rf WRFCHEM_FAILED_*
if [[ $RC = 0 ]]; then
   touch WRFCHEM_SUCCESS_advm_7598     
else
   touch WRFCHEM_FAILED_advm_7598 
   exit
fi
EOF
