rm -rf WRF2DART_SUCCESS_*
rm -rf WRF2DART_FAILED_*
if [[ $RC = 0 ]]; then
   touch WRF2DART_SUCCESS_wf2dt_7598
else
   touch WRF2DART_FAILED_wf2dt_7598 
   exit
fi
EOF
