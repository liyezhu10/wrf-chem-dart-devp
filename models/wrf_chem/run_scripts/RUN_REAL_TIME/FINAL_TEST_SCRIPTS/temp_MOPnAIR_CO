# Run wrf_obs_preprocess
rm -rf pre_*.err
rm -rf pre_*.out
./wrf_dart_obs_preprocess 151045 21600 > index_preprocess 2>&1 
#
export RC=$?     
if [[ -f PRE_SUCCESS ]]; then rm -rf PRE_SUCCESS; fi     
if [[ -f PRE_FAILED ]]; then rm -rf PRE_FAILED; fi          
if [[ $RC = 0 ]]; then
   touch PRE_SUCCESS
else
   touch PRE_FAILED 
   exit
fi
