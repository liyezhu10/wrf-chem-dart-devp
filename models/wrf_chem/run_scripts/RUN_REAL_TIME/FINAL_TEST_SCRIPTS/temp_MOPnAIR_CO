               'ch3cooh -> CH3COOH','cres -> CRESOL','glyald -> GLYALD','mgly -> CH3COCHO',
               'gly -> CH3COCHO','acetol -> HYAC','isop -> ISOP','macr -> MACR'
               'mvk -> MVK',
               'oc -> OC;aerosol','bc -> BC;aerosol'
/
EOF
1> fire_emis.mozc.inp
+ [[ -f job.ksh ]]
+ touch job.ksh
+ RANDOM=7598
+ JOBRND=wrf_fire_3132
+ export JOBRND
+ cat
+ 0<< \EOF
#!/bin/ksh -aeux
#BSUB -P P19010000
#BSUB -n 1
#BSUB -R "span[ptile=8]"    
#BSUB -J wrf_fire_3132
#BSUB -o wrf_fire_3132.out
#BSUB -e wrf_fire_3132.err
#BSUB -W 00:20        
#BSUB -q geyser
#
./fire_emis < fire_emis.mozc.inp > index_fire_emis 2>&1
# 
export RC=$?     
rm -rf WRFCHEM_FIRE_SUCCESS     
rm -rf WRFCHEM_FIRE_FAILED          
if [[ $RC = 0 ]]; then
   touch WRFCHEM_FIRE_SUCCESS
else
   touch WRFCHEM_FIRE_FAILED 
   exit
fi
EOF
