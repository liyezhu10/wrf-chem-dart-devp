#!/bin/ksh -aeux
#BSUB -P P19010000
#BSUB -n 32
#BSUB -R "span[ptile=8]"
#BSUB -J filter_3132
#BSUB -o filter_3132.out
#BSUB -e filter_3132.err
#BSUB -W 5:59
#BSUB -q geyser
#
mpirun.lsf ./filter > index_filter 2>&1 
export RC=$?     
rm -rf FILTER_SUCCESS     
rm -rf FILTER_FAILED          
if [[ $RC = 0 ]]; then
   touch FILTER_SUCCESS
else
   touch FILTER_FAILED 
   exit
fi
EOF
