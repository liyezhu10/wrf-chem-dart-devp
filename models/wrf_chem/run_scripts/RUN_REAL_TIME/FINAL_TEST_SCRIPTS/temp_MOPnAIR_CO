#!/bin/ksh -aeux
#BSUB -P P19010000
#BSUB -n 32                       # number of total (MPI) tasks
#BSUB -R "span[ptile=8]"    # mpi tasks per node
#BSUB -J wrfda_cr_7598                          # job name
#BSUB -o wrfda_cr_7598.out                      # output filename
#BSUB -e wrfda_cr_7598.err                      # error filename
#BSUB -W 0:10               # wallclock time (minutes)
#BSUB -q geyser
#
mpirun.lsf ./da_wrfvar.exe  > index.html 2>&1 
#
export RC=$?     
if [[ -f SUCCESS ]]; then rm -rf SUCCESS_*; fi     
if [[ -f FAILED ]]; then rm -rf FAILED_*; fi          
if [[ $RC = 0 ]]; then
   touch SUCCESS_wrfda_cr_7598
else
   touch FAILED_wrfda_cr_7598 
   exit
fi
EOF
