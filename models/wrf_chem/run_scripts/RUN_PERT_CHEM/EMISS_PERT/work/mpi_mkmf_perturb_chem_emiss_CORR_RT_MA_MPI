cp -f path_names_perturb_chem_emiss_CORR_RT_MA_MPI path_names_perturb_chem_emiss_CORR_RT_MA_MPI.back

sed -e 's;/null_mpi_util;/mpi_util;' path_names_perturb_chem_emiss_CORR_RT_MA_MPI.back >! path_names_perturb_chem_emiss_CORR_RT_MA_MPI
setenv wrapper_arg -w
rm -f path_names_perturb_chem_emiss_CORR_RT_MA_MPI.back

../../../../../../mkmf/mkmf -p perturb_chem_emiss_CORR_RT_MA_MPI.exe -t ../../../../../../mkmf/mkmf.template \
 -a "../../../../../.." ${wrapper_arg} path_names_perturb_chem_emiss_CORR_RT_MA_MPI

exit $status

